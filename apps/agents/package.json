    "@langchain/exa": "^0.1.0",
    "@langchain/google-genai": "^0.2.10",
    "@langchain/groq": "^0.2.2",
    "@langchain/langgraph": "^0.2.73",
    "@langchain/langgraph-sdk": "^0.0.78",
    "@langchain/ollama": "^0.2.0",
    "@langchain/openai": "^0.4.2",
    "@mendable/firecrawl-js": "1.10.1",
