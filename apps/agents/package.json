    "@langchain/langgraph-cli": "^0.0.27",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "@workspace/typescript-config": "workspace:*",
